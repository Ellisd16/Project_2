    <link  href="/assets/css/admin.css" rel="stylesheet" type="text/css" media="screen"/>

 
    <div>
{{> nav}}
</div>
    <div id="submit-form">
        <form id="productForm">
            <div id="map-places-container">
                <div class="form-group">
                        <input id="productName" name="productN" type="text" placeholder="Product Name"></input>
                    </div>
                <div class="form-group">
                        <input id="productDesc" name="productD" type="text" placeholder="Product Description"></input>
                    </div>
                    <div class="form-group">
                        <input id="imgHref" name="imgH" type="text" placeholder="Image Direct Link"></input>
                    </div>
                    <div class="form-group">
                        <input id="audioHref" name="audioH" type="text" placeholder="Audio Link"></input>
                    </div>
                    <div class="form-group">
                        <input id="idinput" name="idinp" type="text" placeholder="ONLY USE IF UPDATING!!!"></input>
                    </div>
            <button type="submit" class="btn btn-default">Submit</button>
            <button type="edit" class="btn btn-default">Edit</button>
        </form>
    </div>
<div>
<ul>
{{!-- database cards go here --}}
        {{#if item}}
        {{#each item}}

    <li class="dbCard"> 
        <p class="infofill">{{ this.dataValues.instName }}</p>
        <p class ="infofill">{{ this.dataValues.descr }}</p>
        <p class ="infofill">{{ this.dataValues.img }}</p>
        <p class ="infofill">{{ this.dataValues.audio }}</p>
        <p class ="infofill">{{ this.dataValues.id }}</p>
        <div class="UD-op"> <a href="/admin/delete/{{this.dataValues.id}}"><img class="deleteBtnImg" src="https://i.imgur.com/WOm4EyA.png"></a></div> 
    </li>
        {{/each}}
        {{/if}}

</ul>
</div>

<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="/assets/js/product.js"></script>